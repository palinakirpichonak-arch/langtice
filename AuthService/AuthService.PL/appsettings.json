{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }, 
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft.AspNetCore.Mvc": "Warning",
                "Microsoft.AspNetCore.Routing": "Warning",
                "Microsoft.AspNetCore.Hosting": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": { "path":  "./logs/log-.txt", "rollingInterval": "Day" }
            }
        ]
    },
  "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "Host=auth-postgres;Port=5432;Database=users;Username=postgres;Password=postgres",
        "Redis": "localhost:6379"
}
}
